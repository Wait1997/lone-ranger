{
  "compilerOptions": {
    "target": "es2016",
    "baseUrl": ".",
    "paths": {"@/*": ["src/*"]},
    "jsx": "react-jsx",
    "module": "commonjs",
    "typeRoots": [],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true                       
  }
